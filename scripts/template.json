{
    "family": "pm-poc-ecs-task-defn",
    "placementConstraints": [],
    "volumes": [],
    "requiresCompatibilities": [
        "EC2"
    ],
    "containerDefinitions": [
        {
            "memoryReservation": 128,
            "name": "pm-poc-ecs-task-defn",
            "mountPoints": [],
            "image": "DOCKER_IMAGE_NAME",
            "essential": true,
            "portMappings": [
                {
                    "protocol": "tcp",
                    "containerPort": 8080,
                    "hostPort": 8090
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-region": "ap-south-1",
                    "awslogs-stream-prefix": "autodeployment",
                    "awslogs-group": "/ecs/ms-autodeploy"
                }
            },
            "memory": 1024,
            "cpu": 0,
            "volumesFrom": []
        }
    ]
}
